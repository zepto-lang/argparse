(load "argparse.zp")

(define x (argparse:handle-args "test" 
              (list (make-hash ["name" "foo"] 
                               ["type" :number]
                               ["default" 10]
                               ["usage" "foo you"])

                    (make-hash ["name" "bar"]
                               ["type" :string]
                               ["usage" "bar me"]))))
(if (hash-map? x)
  (begin
    (write `("foo is: " ,(x "foo")))
    (write `("bar is: " ,(x "bar")))))
